<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <add key="ServiceDescription" value="Hangfire service responsible for running background tasks."/>
    <add key="ServiceDisplayName" value="Hangfire Service"/>
    <add key="ServiceName" value="HangfireService"/>
    <add key="ListenPort" value="9096"/>
  </appSettings>
  <connectionStrings>
    <add name="Hangfire" connectionString="Data Source=(localdb)\MSSQLLocalDB;Initial Catalog=HangfireService;Integrated Security=True;" />
  </connectionStrings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-9.0.0.0" newVersion="9.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>